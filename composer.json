{
    "name": "nuclear/nuclear",
    "description": "Polymorphic CMS powered by Laravel 5.",
    "license": "MIT",
    "keywords": [
        "kenarkose",
        "nuclear",
        "nuclearcms",
        "hierarchy",
        "polymorphic",
        "reactor",
        "cms",
        "laravel",
        "laravel5"
    ],
    "version": "2.0.0-alpha.4",
    "authors": [
        {
            "name": "Hyleeh",
            "email": "egemen@kenarkose.com",
            "homepage": "http://egemenkizilcan.com",
            "role": "Developer / Designer"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "kenarkose/chronicle": "~1.0",
        "kenarkose/files": "~1.0",
        "kenarkose/ownable": "~1.1",
        "kenarkose/settlement": "~1.0",
        "kenarkose/sortable": "~1.0",
        "kenarkose/synthesizer": "~1.0",
        "kenarkose/transit": "~1.2",
        "nuclear/hierarchy": "~1.0",
        "laracasts/flash": "~1.3",
        "laracasts/presenter": "0.2.*",
        "intervention/image": "~2.3",
        "intervention/imagecache": "~2.3",
        "igaster/laravel-theme": "^1.0",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "guzzlehttp/streams": "^3.0",
        "nicolaslopezj/searchable": "^1.5",
        "kris/laravel-form-builder": "^1.6",
        "simexis/oembed": "^1.4",
        "spatie/laravel-backup": "^2.8",
        "barryvdh/laravel-debugbar": "~2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Reactor\\": "app/",
            "Extension\\": "extension/",
            "gen\\": "gen/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}
